  "description": "Приложение позволяет управлять задачами в gitlab",
  "main": ".webpack/main",
  "scripts": {
    "forge:start": "electron-forge start -- --remote-debugging-port=9222",
    "forge:package": "electron-forge package --platform=win32 --arch=x64",
    "forge:make": "electron-forge make --platform=win32 --arch=x64",
    "forge:publish": "electron-forge publish --platform=win32 --arch=x64",
    "codegen:watch": "graphql-codegen --watch",
    "build:all": "run-s codegen forge:package",
    "prettier": "prettier --write src/",
    "codegen": "graphql-codegen",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
